{
  "name": "six-cities",
  "version": "5.0.0",
  "description": "Проект «Шесть городов» от HTML Academy",
  "keywords": [
    "rest",
    "api",
    "node.js"
  ],
  "main": "main.rest.js",
  "scripts": {
    "start": "npm run build && node ./dist/src/main.rest.js",
    "connect": "npm run build && node ./dist/src/connect.db.js",
    "start:dev": "npm run build && node ./dist/src/main.rest.js | pino-pretty --colorize --translateTime SYS:standard",
    "cli": "npm run build && node ./dist/src/cli/main.cli.js",
    "json-server": "json-server --watch src/mocks/mock-server-data.json --port 3000",
    "build": "npm run clean && npm run compile",
    "lint": "sudo eslint src/ --ext .ts",
    "compile": "sudo tsc -p tsconfig.json",
    "clean": "sudo rimraf dist",
    "ts": "ts-node"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/convict": "^6.1.6",
    "@types/convict-format-with-validator": "^6.0.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.0",
    "@types/json2csv": "^5.0.7",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.12",
    "@types/node": "18.17.17",
    "@types/reflect-metadata": "^0.0.5",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "eslint": "8.49.0",
    "eslint-config-htmlacademy": "9.1.1",
    "pino-pretty": "^11.3.0",
    "rimraf": "5.0.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "type": "module",
  "engines": {
    "node": "^18.0.0",
    "npm": ">=8"
  },
  "dependencies": {
    "18": "^0.0.0",
    "@typegoose/typegoose": "^12.9.1",
    "@types/cors": "^2.8.17",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "convict": "^6.2.4",
    "convict-format-with-validator": "^6.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-async-handler": "^1.2.0",
    "http-status-codes": "^2.3.0",
    "inversify": "^6.1.6",
    "jose": "^5.9.6",
    "json-server": "^1.0.0-beta.3",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "mongoose": "^8.8.3",
    "multer": "^1.4.5-lts.1",
    "pino": "^9.5.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "tslib": "^2.8.0"
  }
}
